/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "types": []
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"],
  "paths": {
    "@abp/*": ["../node_modules/abp-ng2-module/src/*"],
    "@app/*": ["./app/*"],
    "@shared/*": ["./app/shared/*"]
  }
}
